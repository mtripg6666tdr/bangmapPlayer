{
  "compilerOptions": {
      "sourceMap": true,
      "target": "es2019",
      "module": "none",
      "moduleResolution": "node",
      "lib": [
          "dom",
          "esnext"
      ],
      "types": [
          "inversify"
      ],
      "noImplicitAny": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "downlevelIteration": true,
      "strict": true,
      "strictNullChecks": true,
      "esModuleInterop": true,

      "outDir": "./dist/",
      "declaration": true,
  },
  "include": [
    "./src",
  ]
}